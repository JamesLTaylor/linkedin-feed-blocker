{
  "content_scripts": [
    {
      "css": [
        "linkedin-feed-blocker.css"
      ],
      "matches": [
        "*://*.linkedin.com/feed/*"
      ]
    },
    {
      "css": [
        "linkedin-badge-blocker.css"
      ],
      "matches": [
        "*://*.linkedin.com/*"
      ]
    },
    {
      "css": [
        "linkedin-network-blocker.css"
      ],
      "matches": [
        "*://*.linkedin.com/mynetwork/*"
      ]
    }
  ],
  "description": "Block as many distractions as possible",
  "icons": {
    "48": "img/icon.png"
  },
  "manifest_version": 2,
  "name": "LinkedIn Feed Blocker",
  "version": "0.1.0"
}
